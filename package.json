        "@vscode/test-electron": "^1.6.2",
        "eslint": "^8.1.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.3",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "axios": "^0.24.0",
        "chrome-remote-interface": "^0.31.1"
